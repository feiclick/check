             File.WriteAllBytes(downLoadPath + "\\Schedule_Day2.docx", response);
             */

            // retriving text from word based on location
            ScheduleTable s = new ScheduleTable();
            string strDateDayActual = s.GetDataFromWord(downLoadPath + "\\AB_Schedule.docx", 2, 1, 1).Trim();          
            string strTime1Actual = s.GetDataFromWord(downLoadPath + "\\AB_Schedule.docx", 2, 12, 1).Trim();
            string strTime2Actual = s.GetDataFromWord(downLoadPath + "\\AB_Schedule.docx", 2, 13, 1).Trim();            
            string strSession1Actual = s.GetDataFromWord(downLoadPath + "\\AB_Schedule.docx", 2, 12, 2).Trim();
            string strSession2Actual = s.GetDataFromWord(downLoadPath + "\\AB_Schedule.docx", 2, 13, 2).Trim();
            string strDescp1Actual = s.GetDataFromWord (downLoadPath + "\\AB_Schedule.docx", 2, 12, 3).Trim();
            string strDescp2Actual = s.GetDataFromWord(downLoadPath + "\\AB_Schedule.docx", 2, 13, 3).Trim();

            // ASSERT
            Assert.AreEqual(strDateDayExpected, strDateDayActual);
            Assert.AreEqual(strTime1Expected, strTime1Actual);
            Assert.AreEqual(strTime2Expected, strTime2Actual);
            Assert.AreEqual(strSession1Expected, strSession1Actual);
            Assert.AreEqual(strSession2Expected, strSession2Actual);
            Assert.AreEqual(strDescp1Expected, strDescp1Actual);
            Assert.AreEqual(strDescp2Expected, strDescp2Actual);

        }      

